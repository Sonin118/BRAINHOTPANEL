-- WALLS (palavras especÃ­ficas incluindo estruturas de plots)
local WALL_KEYWORDS = {"wall","parede","barrier","invisible","barreira","block","border","gate","bar","kill","killbrick","side"}
-- Adicionado "side" para detectar Side 1, Side 2, Side 3, etc.

-- Palavras que NÃƒO devem ser afetadas (estruturas crÃ­ticas)
local PROTECTED_KEYWORDS = {"floor","chÃ£o","solo","ground","terrain","map","mundo","world","spawn","teleport","portal","platform","plataforma"}

local ENABLE_REMOVE_WALLS = false
local WALL_TAG             = "NoWallProcessed"
