if ENABLE_ANTI_AURA_LAG then print("â†’ Anti-lag para auras/efeitos ativado") end
if ENABLE_STRIP_SKINS then print("â†’ RemoÃ§Ã£o otimizada de skins/roupas/acessÃ³rios") end
if ENABLE_ADMIN_PANEL then print("â†’ Painel de administraÃ§Ã£o ativado (F9 para abrir)") end
if ENABLE_NO_WALLS then
    print("â†’ Walls/estruturas processadas. Intervalo: "..WALL_SCAN_INTERVAL.."s")
    print("â†’ TransparÃªncia: "..(WALL_TRANSPARENCY * 100).."%")
    print("â†’ "..(ENABLE_WALL_HACK and "WALL HACK ATIVO" or ENABLE_REMOVE_WALLS and "MODO REMOVER ATIVO" or "Estruturas transparentes (COM COLISÃƒO)"))
    print("â†’ Detecta: walls, barriers, blocks, Side 1/2/3 (PROTEÃ‡ÃƒO: chÃ£o/solo/mapa)")
end
print("â†’ Sistema de FPS Ãºnico e correto")
